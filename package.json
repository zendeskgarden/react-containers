    "rollup": "3.29.4",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-license": "3.1.0",
    "rollup-plugin-typescript2": "0.36.0",
    "storybook": "7.4.5",
    "styled-components": "6.0.8",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
