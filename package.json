{
  "private": true,
  "name": "@zendeskgarden/react-containers",
  "version": "0.0.0",
  "scripts": {
    "build": "lerna run build --stream --concurrency=1",
    "build:demo": "storybook build -o demo",
    "format": "prettier-package-json --write && npm run -- format:package_json --write && npm run -- format:js --write && npm run -- format:md --write",
    "format:all": "prettier-package-json --list-different && npm run -- format:package_json --list-different && npm run -- format:js --check && npm run -- format:md --check",
    "format:js": "prettier --log-level warn '{packages,utils}/**/*.{js,mjs,ts,tsx}' '!packages/.template' '!packages/**/dist/**'",
    "format:md": "prettier --log-level warn 'packages/**/!(CHANGELOG)*.md'",
    "format:package_json": "lerna exec -- prettier-package-json",
    "postinstall": "husky install",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint .storybook/ packages/ utils/ --ext js,mjs,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint README.md packages/*/src/**/*.md packages/*/src/*.md packages/*/README.md",
    "new": "utils/scripts/new.mjs",
    "prepare": "npm run build",
    "start": "storybook dev --no-version-updates -p 6006",
    "start:demo": "npm run build:demo && live-server demo",
    "tag": "lerna version --conventional-commits --force-git-tag",
    "test": "jest --config=utils/test/jest.config.js",
    "test:ci": "npm test -- --coverage --runInBand",
    "test:watch": "npm test -- --watch",
    "version": "npm run build && git add -A"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.0",
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-transform-object-assign": "7.22.5",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.0",
    "@babel/runtime": "7.23.1",
    "@brodybits/rollup-plugin-size-snapshot": "0.15.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-replace": "5.0.2",
    "@storybook/addon-a11y": "7.4.5",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/react": "7.4.5",
    "@storybook/react-webpack5": "7.4.5",
    "@swc/core": "1.3.91",
    "@swc/jest": "0.2.29",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/prop-types": "15.7.8",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@zendeskgarden/eslint-config": "35.0.0",
    "@zendeskgarden/react-theming": "8.71.0",
    "@zendeskgarden/scripts": "2.0.2",
    "@zendeskgarden/tailwindcss": "3.1.1",
    "acorn-jsx": "5.3.2",
    "autoprefixer": "10.4.16",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "classnames": "2.3.2",
    "commander": "11.0.0",
    "core-js": "3.33.0",
    "coveralls": "3.1.1",
    "envalid": "8.0.0",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "execa": "8.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
